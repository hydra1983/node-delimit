{
  "name": "delimit",
  "version": "0.1.1",
  "description": "Convert delimited files from one format to another",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "find test/ -name '*.js' | while read file; do echo $file; mocha $file; [ $? -ne 0 ] && { exit $?; }; done; exit 0"
  },
  "bin": {
    "delimit": "./bin/delimit.js"
  },
  "author": "Trevor Senior <trevor@tsenior.com> (http://tsenior.com/)",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "when": "~2.7.0",
    "lodash": "~2.4.1",
    "byline": "~4.1.1",
    "temp": "~0.6.0",
    "main": "0.0.21",
    "split": "~0.3.0",
    "fs-extra": "~0.8.1"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "chai-as-promised": "~4.1.0",
    "mocha": "~1.17.0",
    "mocha-as-promised": "~2.0.0"
  },
  "keywords": [
    "delimit",
    "convert",
    "csv",
    "tsv",
    "xls",
